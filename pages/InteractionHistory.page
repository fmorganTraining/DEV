        var mobileIcon = '<svg viewbox="-2 -2 54 104" width="50px" height="50px"><rect x="0" y="0" rx="5" ry="5" width="60" height="100" style="fill:black;stroke:black;stroke-width:2;" /><rect x="15" y="0" rx="2" ry="2" width="30" height="5" style="fill:white;stroke:black;stroke-width:2;" /><rect x="5" y="5" rx="2" ry="2" width="50" height="80" style="fill:white;stroke:black;stroke-width:2;" /><circle cx="30" cy="92" r="5" stroke="black" stroke-width="2" fill="white" /></svg>';
        window.channel = {
            web:["Banking Web Site"],
            mobile:["Customer First iOS app","Mobile iOS Banking"],
            email:["Email"],
            assisted:["CallCenter"],
            physical:["Banking System","Branch","Localz Branch"],
            social:["Facebook","Twitter"]
        }
        var filterCondition = "Clear";
        var searchCondition = "";
        var showChannel = {
            web:true,
            mobile:true,
            outbound:true,
            assisted:true,
            physical:true,
            social:true            
        }
        var hasChannels = {
            web:false,
            mobile:false,
            outbound:false,
            assisted:false,
            physical:false,
            social:false            
        }
        // Cached list of all rows (in html form)
        var activityHistoryRows = [];
        // Checks if we have finished loading and populating the list
        var hasPopulated = false;
        function processActivityHistory() {
            // All activity types only
            var headers = document.querySelectorAll("thead>tr>th");
            var lifecycleStages;
            for (var i = 0; i < headers.length; i++) {
                if (headers[i].innerText === "Lifecycle Stage") {
                    lifecycleStages = document.querySelectorAll('tr > td:nth-child('+(i+1)+')');
                }
            }
            // For each activity
            lifecycleStages.forEach(function (row, x) {
                var searchable = [];
                row.parentNode.querySelectorAll("td").forEach(function (ele) {
                    searchable.push(ele.innerText);
                });
                Object.keys(channel).forEach(function (ele) {
                    channel[ele].forEach(function (touchpoint) {
                        if (touchpoint === row.parentElement.childNodes[1].innerText) {
                            searchable.push(ele);
                        }
                    });
                });
                activityHistoryRows.push({
                    type: row.innerText,
                    row: row.parentElement,
                    search: searchable,
                    channel: searchable.slice(-1)[0]
                });
            });
            hasPopulated = true;
        }

        function filterListByType(event) {
            if (!!event) {
                filterCondition = event.target.innerText.trim();
            }            
            applyFilters();
        }

        function filterByChannel(event) {
            Object.keys(showChannel).forEach(function (ele) {
                showChannel[ele] = false;
            })
            document.querySelectorAll("input[name='channel']:checked").forEach(function (ele) {
                showChannel[ele.labels[0].innerHTML.trim().toLowerCase()] = true
            });
            applyFilters();
        }
        function applySearch(event) {
            if (!!event) {
                searchCondition = event.target.value;
            }
            applyFilters();
        }
        function addFilterChannelButtons() {
            activityHistoryRows.forEach(function (ele) {
                hasChannels[ele.channel] = true;
            });
            document.querySelector("div.channels").innerHTML = "";

            Object.keys(hasChannels).forEach(function (ele) {
                if (hasChannels[ele]) {
                    var propCaseChannel = (ele[0] + ele.slice(1));
                    document.querySelector("div.channels").innerHTML += '<div class="form-check form-check-inline"><input type="checkbox" name="channel" id="select'+propCaseChannel+'" checked="true" onchange="filterByChannel()" class="form-check-label"/><label for="select'+propCaseChannel+'" class="form-check-label">'+propCaseChannel+'</label></div>';
                }
            });
        }
        function applyFilters() {
            var matches = activityHistoryRows.filter(function (ele) {
                return (filterCondition === "Clear" || ele.type === filterCondition || (filterCondition === "Other" && ele.type === "")) && (ele.search.some(function (searchEle) {return RegExp(searchCondition,"i").test(searchEle)}) || searchCondition === "") && (showChannel[ele.channel.toLowerCase()]);
            });

            $("#activity-view-container").empty();
            matches.forEach(function(ele) {
                $("#activity-view-container").append(ele.row);
            });
        }
    </script>
