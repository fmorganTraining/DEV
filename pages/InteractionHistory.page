    <script>

        var emailIcon = '<svg viewbox="-2 -2 204 104" width="75px" height="50px"><rect x="0" y="0" width="150" height="100" style="fill:none;stroke:black;stroke-width:4;" /><line x1="0" y1="0" x2="75" y2="50" style="stroke:rgb(0,0,0);stroke-width:4" /><line x1="75" y1="50" x2="150" y2="0" style="stroke:rgb(0,0,0);stroke-width:4" /></svg>';
