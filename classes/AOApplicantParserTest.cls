	@IsTest
    private static void testHappyPath() {
        String testData = '<?xml version="1.0" encoding="UTF-8"?><Root><AvokaSmartForm><Applicant><Contact><FirstName>Kenneth</FirstName><LastName>Peeler</LastName><Email>lbaxter@avoka.com</Email><DateOfBirth>1949-04-25</DateOfBirth><Ssn>666-18-0648</Ssn><MaskedSsn>0648</MaskedSsn><ApplicantPersona>unknown</ApplicantPersona><PersonNumber>1234678</PersonNumber><Phone>310-427-5901</Phone><MobilePhone>310-427-5902</MobilePhone><WorkPhone>310-427-5903</WorkPhone><MailingStreet>516 E Double St Apt#2</MailingStreet><MailingStreet2>#2</MailingStreet2><MailingCity>Carson</MailingCity><MailingState>CA</MailingState><MailingPostalCode>907452121</MailingPostalCode><MailingCountry>USA</MailingCountry><MailingCountryCode>USA</MailingCountryCode><IDNumber>123456789</IDNumber><IDType>Drivers License</IDType><IDIssueState>CO</IDIssueState><IDIssueDate>2010-01-01</IDIssueDate><IDExpiryDate>2020-01-01</IDExpiryDate></Contact><AccountList/></Applicant></AvokaSmartForm></Root>';
        XPath xpath = new XPath(testData);

        Dom.XmlNode applicantNode = xpath.findFirst('/Root/AvokaSmartForm/Applicant');
        List<Map<String,Object>> applicantPayloads = new List<Map<String,Object>>();
        Map<String, Object> v0ApplicantPayload = new Map<String, Object>();
        v0ApplicantPayload.put(AOConstants.XML_APPLICANT_KEY, applicantNode);
        applicantPayloads.add(v0ApplicantPayload);
        Map<String, Object> v1ApplicantPayload = new Map<String, Object>();
        v1ApplicantPayload.put(AOConstants.XML_APPLICANT_KEY, applicantNode);
        v1ApplicantPayload.put(AOConstants.XML_VERSION, AOConstants.FormXmlVersion.V1);
        applicantPayloads.add(v1ApplicantPayload);
        Map<String, Object> v2ApplicantPayload = new Map<String, Object>();
        v2ApplicantPayload.put(AOConstants.XML_APPLICANT_KEY, applicantNode);
        v2ApplicantPayload.put(AOConstants.XML_VERSION, AOConstants.FormXmlVersion.V2);
        applicantPayloads.add(v2ApplicantPayload);
        
        for (Map<String, Object> applicantPayload : applicantPayloads) {
            AOApplicationInfo appInfo = new AOApplicationInfo();
            AOApplicantParser applicantParser = new AOApplicantParser();
            applicantParser.parsePayload(applicantPayload, appInfo);

            System.assert(appInfo.personNumber != null, 'Person number was null.');
            System.assert(appInfo.phone != null, 'Phone was null.');
            System.assert(appInfo.mobilePhone != null, 'Mobile phone was null.');
            System.assert(appInfo.workPhone != null, 'Work phone was null.');
            System.assert(appInfo.email != null, 'Email was null.');
            System.assert(appInfo.taxId != null, 'TaxId was null.');
            System.assert(appInfo.firstName != null, 'First name was null.');
            System.assert(appInfo.lastName != null, 'Last name was null.');
            System.assert(appInfo.mailingStreet != null, 'Mailing street was null.');
            System.assert(appInfo.mailingStreet2 != null, 'Mailing street 2 was null.');
            System.assert(appInfo.mailingCity != null, 'Mailing city was null.');
            System.assert(appInfo.mailingState != null, 'Mailing state was null.');
            System.assert(appInfo.mailingPostalCode != null, 'Mailing postal code was null.');
            System.assert(appInfo.mailingCountryCode != null, 'Mailing postal code was null.');
            System.assert(appInfo.birthDate != null, 'Birthdate was null.');
            System.assert(appInfo.idNumber != null, 'ID Number was null.');
            System.assert(appInfo.idType != null, 'ID type was null.');
            System.assert(appInfo.idExpiryDate != null, 'ID expiry date was null.');
            System.assert(appInfo.idIssueDate != null, 'ID issue date was null.');
            System.assert(appInfo.idIssueState != null, 'ID state was null.');
            
            Integer expectedErrorCount = 0;
            Integer actualErrorCount = appInfo.errors.size();
            System.assertEquals(expectedErrorCount, actualErrorCount, 'Error: ' + appInfo.errors);
        }

    }

